class M99_ff{
   public static void main(String[] args) {
	  int i, j;
	  System.out.println("M99");
	  for(i=1; i<=9; i=i+1) {
         System.out.print(i+": ");
		 for(j=1; j<=9; j=j+1) {
		    System.out.println(i+"*"+j+"="+i*j);
		 }
		 System.out.println("");
   	  }
	  System.out.println("OK");
   }
}   